{
  "expo": {
    "name": "Christmas Games",
    "slug": "christmas-games",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6ed3ff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6ed3ff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "shortName": "Xmas Games",
      "display": "standalone",
      "themeColor": "#0b1021",
      "backgroundColor": "#6ed3ff",
      "lang": "en"
    }
  }
}
